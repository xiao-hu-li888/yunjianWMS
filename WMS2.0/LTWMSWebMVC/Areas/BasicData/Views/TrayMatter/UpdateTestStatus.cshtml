@model LTWMSWebMVC.Areas.BasicData.Data.TrayMatterModel

@{
    ViewBag.Title = "Add";
}
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

<fieldset>
    <legend>登录用户</legend>
    @Html.HiddenFor(model => model.guid)
    @Html.HiddenFor(model => model.OldRowVersion)
    <div class="form-group form-border">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1">
            <span class="necessary">*</span> @Html.LabelFor(model => model.lot_number)
        </label>
        <div class="col-sm-9">
            @Html.EditorFor(model => model.lot_number)
            @Html.ValidationMessageFor(model => model.lot_number)
        </div>
        <div class="clear"></div>
    </div>
    <div class="form-group form-border">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1">@Html.LabelFor(model => model.new_matter_barcode)</label>
        <div class="col-sm-9">
            @Html.EditorFor(model => model.new_matter_barcode)
            @Html.ValidationMessageFor(model => model.new_matter_barcode)
        </div>
        <div class="clear"></div>
    </div>
    <div class="form-group form-border">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1">@Html.LabelFor(model => model.test_status)</label>
        <div class="col-sm-9">
            @Html.EditorFor(model => model.test_status)
            @Html.ValidationMessageFor(model => model.test_status)
        </div>
        <div class="clear"></div>
    </div>

    <p>
        <button class="btn btn-sm btn-info" type="submit">
            <i class="ace-icon fa fa-check bigger-110"></i>
            保存设置
        </button>
    </p>
</fieldset>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
