<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/Content/assets/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/assets/font-awesome/4.5.0/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/Content/assets/css/ace.min.css" />
    <link href="~/Content/my-min.css" rel="stylesheet" />
 
    @Styles.Render("~/Content/jqueryui")
    @Styles.Render("~/Content/page")
   
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
   
    <div class="body-content">
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/Scripts/My97DatePicker/js")
    @Scripts.Render("~/Scripts/main")
    <script src="~/Scripts/speech.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>