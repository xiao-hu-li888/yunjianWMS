@model LTWMSWebMVC.Areas.Bills.Data.bill_stockin_detail_traymatterModel

@{
    ViewBag.Title = "Add";
}
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

<fieldset>
    <legend></legend>
    @if (ViewBag.isUpdate != null && ViewBag.isUpdate)
    {
        @Html.HiddenFor(model => model.guid)
        @Html.HiddenFor(model => model.OldRowVersion)
    }
    <div class="form-group form-border">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1">@Html.LabelFor(model => model.traybarcode)</label>
        <div class="col-sm-9">
            @Html.DisplayFor(model => model.traybarcode)
            @Html.HiddenFor(model => model.traybarcode)
        </div>
        <div class="clear"></div>
    </div>
    <div class="form-group form-border">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1">  @Html.LabelFor(model => model.matter_name)  </label>
        <div class="col-sm-9">
            @Html.DisplayFor(model => model.matter_code) / @Html.DisplayFor(model => model.matter_name)
            @Html.ValidationMessageFor(model => model.matter_name)
        </div>
        <div class="clear"></div>
    </div>

    <div class="form-group form-border">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> @Html.LabelFor(model => model.dest_shelfunits_pos)  </label>
        <div class="col-sm-9">
            @Html.DisplayFor(model => model.dest_shelfunits_pos)
            @Html.HiddenFor(model => model.dest_shelfunits_pos)
        </div>
        <div class="clear"></div>
    </div>

    <div class="form-group form-border">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> @Html.LabelFor(model => model.lot_number)  </label>
        <div class="col-sm-9">
            @Html.DisplayFor(model => model.lot_number)
            @Html.HiddenFor(model => model.lot_number)

        </div>
        <div class="clear"></div>
    </div>

    <div class="form-group form-border">
        <label class="col-sm-3 control-label no-padding-right" for="form-field-1"> @Html.LabelFor(model => model.number)  </label>
        <div class="col-sm-9">
            @Html.EditorFor(model => model.number)
            @Html.HiddenFor(model => model.number)
        </div>
        <div class="clear"></div>
    </div>


    <p>
        <button class="btn btn-sm btn-info" type="submit">
            <i class="ace-icon fa fa-check bigger-110"></i>
            修改数量
        </button>
    </p>
</fieldset>
} 